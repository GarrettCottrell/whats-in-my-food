<%= @json[:totalHits] %>

<% @foods.first(10).each_with_index do |food, index| %>
    <ul class="food-<%= index %>">
      <li class="description"><%= food.description %>
      <li class="brandowner"><%= food.brand_owner %>
      <li class="ingredients"><%= food.ingredients %>
      <li class="gtinUpc"><%= food.upc %>
    </ul>
  <% end %>